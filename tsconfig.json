{
  "extends": ["@tsconfig/strictest/tsconfig.json"],
  "include": ["package.json"],
  "compilerOptions": {
    "module": "Node16",
    "moduleResolution": "Node16",
    "target": "ES2022",
    "lib": ["ES2023", "DOM", "DOM.Iterable"],
    "composite": true,
    "resolveJsonModule": true,
    "stripInternal": true,
    "exactOptionalPropertyTypes": false,
  },
}
